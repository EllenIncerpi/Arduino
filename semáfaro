// C++ code
//
void setup()
{
  pinMode(7, OUTPUT); 
  pinMode(3, OUTPUT);
  pinMode(11, OUTPUT);
}


void loop()

{
  digitalWrite(11, HIGH);
  delay(5000); // Wait for 1000 millisecond(s)
  digitalWrite(11, LOW);
  

  digitalWrite(7, HIGH);
  delay(2000); // Wait for 1000 millisecond(s)
  digitalWrite(7, LOW);
  


  digitalWrite(3, HIGH);
  delay(2000); // Wait for 1000 millisecond(s)
  digitalWrite(3, LOW);
}
